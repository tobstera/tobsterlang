<Root module="sum">
    <Func name="add" x="Int32" y="Int32" returns="Int32">
        <!-- Last statement of function is implicitly returned if return type is not void -->
        <Add>
            <Load name="x"/>
            <Load name="y"/>
        </Add>
    </Func>

    <Func name="ZdraveitePriqteliAzSumTobstera" returns="Void">
        <Call name="printf">
            <Value type="String" raw="true">%d\n</Value>
            <Call name="add">
                <Value type="Int32">400</Value>
                <Value type="Int32">20</Value>
            </Call>
        </Call>
    </Func>
</Root>